#include <stdio.h>
int main() {
    int arr[100], n, key;
    printf("Enter number of elements: ");
    scanf("%d", &n);
    printf("Enter elements:\n");
    for (int i = 0; i < n; i++)
        scanf("%d", &arr[i]);
    printf("Enter number to search: ");
    scanf("%d", &key);
    for (int i = 0; i < n; i++) {
        if (arr[i] == key) {
            printf("Found at position %d\n", i + 1);
            return 0;
        }
    }
    printf("Not found\n");
    return 0;
}

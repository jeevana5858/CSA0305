#include <stdio.h>
int main() {
    int a[50], b[50], c[100];
    int n1, n2, i, j, k = 0;
    printf("Enter size of first array: ");
    scanf("%d", &n1);
    printf("Enter elements of first array: ");
    for (i = 0; i < n1; i++)
        scanf("%d", &a[i]);
    printf("Enter size of second array: ");
    scanf("%d", &n2);
    printf("Enter elements of second array: ");
    for (j = 0; j < n2; j++)
        scanf("%d", &b[j]);
    for (i = 0; i < n1; i++)
        c[k++] = a[i];
    for (j = 0; j < n2; j++)
        c[k++] = b[j];
    printf("Merged Array: ");
    for (i = 0; i < k; i++)
        printf("%d ", c[i]);
    printf("\n");
    return 0;
}
